{
  "schema_version": 1,
  "quilt_loader": {
    "group": "compasses",
    "id": "expandedstorage",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "compasses.expandedstorage.quilt.QuiltMain",
      "client_init": "compasses.expandedstorage.quilt.QuiltClient",
      "fabric-datagen": "compasses.expandedstorage.thread.datagen.Main",
      "emi": "compasses.expandedstorage.thread.client.EmiCompat",
      "jei_mod_plugin": "compasses.expandedstorage.common.client.JeiCompat",
      "modmenu": "compasses.expandedstorage.thread.client.ModMenuCompat",
      "rei_client": "compasses.expandedstorage.thread.client.ReiCompat"
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
        "versions": ">=0.18.6"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20-beta.1"
      },
      {
        "id": "org.quiltmc:quilted_fabric_api",
        "versions": ">=6.0.0-beta.2"
      }
    ],
    "metadata": {
      "name": "Expanded Storage",
      "description": "Adds new storage blocks with higher capacities.",
      "contributors": { "Compasses": "Owners" },
      "contact": {
        "homepage": "https://modrinth.com/mod/expanded-storage",
        "issues": "https://gitlab.com/BucketOfCompasses/expanded-storage/-/issues",
        "sources": "https://gitlab.com/BucketOfCompasses/expanded-storage"
      },
      "license": [ ],
      "icon": "icon.png"
    }
  },
  "mixin": [
    "expandedstorage-common.mixins.json",
    "expandedstorage-thread.mixins.json"
  ]
}
