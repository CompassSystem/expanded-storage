{
  "schema_version": 1,
  "quilt_loader": {
    "group": "ninjaphenix",
    "id": "expandedstorage",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "ninjaphenix.expandedstorage.quilt.Main",
      "fabric-datagen": "ninjaphenix.expandedstorage.quilt.datagen.Main"
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
        "versions": "~0.16.0-beta.12"
      },
      {
        "id": "ninjaphenix:ninjaphenix_container_lib",
        "versions": "~1.3.4"
      },
      {
