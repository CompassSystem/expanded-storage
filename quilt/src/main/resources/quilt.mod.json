{
  "schema_version": 1,
  "quilt_loader": {
    "group": "ellemes",
    "id": "expandedstorage",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": ["ellemes.container_library.quilt.QuiltMain", "ellemes.expandedstorage.quilt.QuiltMain"],
      "client_init": ["ellemes.container_library.quilt.QuiltClient"],
      "fabric-datagen": "ellemes.expandedstorage.thread.datagen.Main",
      "modmenu": "ellemes.expandedstorage.thread.client.ModMenuCompat",
      "rei_client": "ellemes.container_library.thread.client.ReiCompat",
      "emi": "ellemes.container_library.thread.client.EmiCompat"
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
        "versions": ">=0.17.6"
      },
      {
        "id": "minecraft",
        "versions": "1.19.2"
      },
      {
        "id": "quilt_base",
        "versions": ">=3.0.0-beta.20"
      },
      {
        "id": "quilted_fabric_api_base",
        "versions": ">=4.0.0-beta.19"
      }
    ],
    "breaks": [
      {
        "id": "ellemes_container_lib",
        "versions": "*",
        "reason": "Now included in codebase, does not need to be installed separately."
      }
    ],
    "metadata": {
      "name": "Expanded Storage",
      "description": "Adds new storage blocks with higher capacities.",
      "contributors": {
        "Ellemes": "Owner"
      },
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/expanded-storage/",
        "issues": "https://github.com/Ellemes/expanded-storage/issues/",
        "sources": "https://github.com/Ellemes/ExpandedStorage/"
      },
      "license": [
      ],
      "icon": "icon.png"
    }
  },
  "mixin": [
    "ellemes-container-library-thread.mixin.json",
    "expandedstorage-common.mixin.json",
    "expandedstorage-thread.mixin.json",
    "expandedstorage-quilt.mixin.json"
  ]
}
