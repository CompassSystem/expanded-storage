{
  "schema_version": 1,
  "quilt_loader": {
    "group": "ellemes",
    "id": "expandedstorage",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": ["ellemes.container_library.quilt.QuiltMain", "ellemes.expandedstorage.quilt.QuiltMain"],
      "client_init": ["ellemes.container_library.quilt.QuiltClient"],
      "fabric-datagen": "ellemes.expandedstorage.thread.datagen.Main",
      "rei_client": "ellemes.container_library.thread.client.ReiCompat",
      "modmenu": "ellemes.expandedstorage.thread.client.ModMenuCompat",
      "emi": "ellemes.container_library.thread.client.EmiCompat",
      "jei_mod_plugin": "ellemes.container_library.client.JeiCompat"
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
        "versions": ">=0.17.10"
      },
      {
        "id": "minecraft",
        "versions": "1.19.2"
      },
      {
        "id": "org.quiltmc:quilted_fabric_api",
        "versions": ">=4.0.0-beta.19"
      }
    ],
    "breaks": [
      {
        "id": "ellemes_container_lib",
        "versions": "*",
        "reason": "Now included in codebase, does not need to be installed separately."
      }
    ],
    "metadata": {
      "name": "Expanded Storage",
      "description": "Adds new storage blocks with higher capacities.",
      "contributors": {
        "Bucket of Compasses": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/expanded-storage",
        "issues": "https://gitlab.com/BucketOfCompasses/expanded-storage/-/issues",
        "sources": "https://gitlab.com/BucketOfCompasses/expanded-storage"
      },
      "license": [
      ],
      "icon": "icon.png"
    }
  },
  "mixin": [
    "ellemes-container-library-thread.mixins.json",
    "expandedstorage-common.mixins.json",
    "expandedstorage-thread.mixins.json"
  ]
}
