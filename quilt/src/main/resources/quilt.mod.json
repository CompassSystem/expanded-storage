{
  "schema_version": 1,
  "quilt_loader": {
    "group": "ellemes",
    "id": "expandedstorage",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "ellemes.expandedstorage.quilt.QuiltMain",
        "ellemes.container_library.quilt.QuiltMain"
      ],
      "client_init": [
        "ellemes.expandedstorage.quilt.QuiltClient",
        "ellemes.container_library.quilt.QuiltClient"
      ],
      "fabric-datagen": "ellemes.expandedstorage.thread.datagen.Main",
      "rei_client": "ellemes.container_library.thread.client.ReiCompat",
      "modmenu": [
        "ellemes.expandedstorage.thread.client.ModMenuCompat",
        "ellemes.container_library.thread.client.ModMenuCompat"
      ]
    },
    "depends": [
      {
        "id": "org.quiltmc:quilt_loader",
        "versions": ">=0.16.0-beta.12"
      },
      {
        "id": "minecraft",
        "versions": "~1.18.2"
      },
      "org.quiltmc.qsl.block:quilt_block_extensions",
      "org.quiltmc.qsl.core:quilt_networking",
      "org.quiltmc.qsl.core:quilt_registry",
      "org.quiltmc.qsl.core:quilt_resource_loader",
      "org.quiltmc.qsl.item:quilt_item_group",
      "org.quiltmc.qsl.gui:quilt_tooltip",
      "org.quiltmc.quilted-fabric-api:quilted_fabric_api_base",
      "org.quiltmc.quilted-fabric-api:quilted_fabric_rendering_v1",
      "org.quiltmc.quilted-fabric-api:quilted_fabric_textures_v0",
      "org.quiltmc.quilted-fabric-api:quilted_fabric_transfer_api_v1"
    ],
    "metadata": {
      "name": "Expanded Storage",
      "description": "Adds new storage blocks with higher capacities.",
      "contributors": {
        "Ellemes": "Owner"
      },
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/expanded-storage/",
        "issues": "https://github.com/Ellemes/expanded-storage/issues/",
        "sources": "https://github.com/Ellemes/ExpandedStorage/"
      },
      "license": [
      ],
      "icon": "icon.png"
    }
  },
  "mixin": [
    "expandedstorage-common.mixin.json",
    "expandedstorage-thread.mixin.json",
    "expandedstorage-quilt.mixin.json",
    "ellemes-container-library-thread.mixins.json"
  ]
}
