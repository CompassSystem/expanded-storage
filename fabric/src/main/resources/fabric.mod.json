    "fabric-datagen": [
      "ellemes.expandedstorage.thread.datagen.Main"
    ],
    "modmenu": [
      "ellemes.expandedstorage.thread.client.ModMenuCompat"
    ],
    "rei_client": [
      "ellemes.expandedstorage.thread.client.ReiCompat"
    ],
    "emi": [
