{
  "schemaVersion": 1,
  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": "1.18.2",
    "fabric": ">=0.67.0+1.18.2"
  },
  "breaks": {
    "ellemes_container_lib": "*"
  },
  "entrypoints": {
    "main": [
      "ellemes.expandedstorage.fabric.FabricMain"
    ],
    "client": [
      "ellemes.expandedstorage.fabric.FabricClient"
