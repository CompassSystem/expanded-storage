    "expandedstorage-common.mixins.json",
    "expandedstorage-thread.mixins.json"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/expanded-storage",
    "issues": "https://gitlab.com/BucketOfCompasses/expanded-storage/-/issues",
