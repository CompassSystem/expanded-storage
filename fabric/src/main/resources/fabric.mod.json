{
  "schemaVersion": 1,
  "depends": {
    "fabricloader": ">=0.14.10",
    "minecraft": "1.19.3",
    "fabric-api": ">=0.65.2+1.19.3"
  },
  "breaks": {
    "ellemes_container_lib": "*"
  },
  "entrypoints": {
    "main": [
      "ellemes.container_library.fabric.FabricMain",
      "ellemes.expandedstorage.fabric.FabricMain"
    ],
    "client": [
      "ellemes.container_library.fabric.FabricClient"
    ],
    "fabric-datagen": [
      "ellemes.expandedstorage.thread.datagen.Main"
    ],
    "modmenu": [
      "ellemes.expandedstorage.thread.client.ModMenuCompat"
    ],
    "rei_client": [
      "ellemes.container_library.thread.client.ReiCompat"
    ],
    "emi": [
      "ellemes.container_library.thread.client.EmiCompat"
    ],
    "jei_mod_plugin": [
      "ellemes.container_library.client.JeiCompat"
    ]
  },
  "environment": "*",
  "authors": ["Bucket of Compasses"],
  "mixins": [
    "ellemes-container-library-thread.mixins.json",
    "expandedstorage-common.mixins.json",
    "expandedstorage-thread.mixins.json"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/expanded-storage",
    "issues": "https://gitlab.com/BucketOfCompasses/expanded-storage/-/issues",
    "sources": "https://gitlab.com/BucketOfCompasses/expanded-storage"
  },
  "icon": "icon.png",
  "id": "expandedstorage",
  "name": "Expanded Storage",
  "version": "${version}"
}
